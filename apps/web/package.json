{
  "name": "@epoch/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.ts,.vue src",
    "lint:style": "stylelint \"src/**/*.{vue,css,scss}\" --fix --allow-empty-input",
    "format": "prettier --write \"src/**/*.{js,ts,vue}\"",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "version-polling": "^1.3.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@epoch/kit": "workspace:*",
    "@epoch/tsconfig": "workspace:*",
    "@epoch/vite": "workspace:*",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "globals": "^16.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.87.0",
    "terser": "^5.39.0",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vconsole": "^3.15.1",
    "vite": "^6.3.4",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-inspect": "^11.0.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vue-tsc": "^2.2.10"
  }
}